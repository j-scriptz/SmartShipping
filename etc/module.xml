<?xml version="1.0"?>
<!--
Jscriptz SmartShipping - Combined Shipping Module

Provides:
- USPS, UPS, FedEx carriers with OAuth2 APIs
- Smart Shipping UI (transit times, badges, cutoffs)
- Luma (Knockout.js) and Hyva (Alpine.js) support
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <module name="Jscriptz_SmartShipping" setup_version="1.0.0">
        <sequence>
            <module name="Magento_Quote"/>
            <module name="Magento_Shipping"/>
            <module name="Magento_Checkout"/>
        </sequence>
    </module>
</config>
