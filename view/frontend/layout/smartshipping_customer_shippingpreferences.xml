<?xml version="1.0"?>
<!--
/**
 * Jscriptz SmartShipping - Customer Shipping Preferences Page Layout
 */
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Jscriptz\SmartShipping\Block\Customer\Account\ShippingPreferences"
                   name="customer.shipping.preferences"
                   template="Jscriptz_SmartShipping::customer/account/shipping_preferences.phtml"
                   cacheable="false"/>
        </referenceContainer>
    </body>
</page>
